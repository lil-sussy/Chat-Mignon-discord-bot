{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Bot xd",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "debug:bot"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug fetlife test",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "debug:fetlife"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug further fetlife test",
      "program": "${workspaceFolder}/src/tests/fetlifeFuthertest.js",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug pupeeter fetlife test",
      "program": "${workspaceFolder}/src/tests/fetlifepuppeteer.js",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "node",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "request": "launch",
      "name": "Debug bot TypeScript 2",
      "program": "${workspaceFolder}/src/bot.ts",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "sourceMaps": true
    }
  ]
}